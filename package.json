{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "jasmine": "^2.4.1",
    "jshint": "^2.9.3",
    "jsxhint": "^0.15.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "mocha": "^3.0.2",
    "mockery": "^1.7.0",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "phaser-mock": "git+https://github.com/richardhealy/phaser-mock.git",
    "proxyquire": "^1.7.10",
    "proxyquireify": "^3.2.1",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.6",
    "webpack-node-externals": "^1.4.3"
  },
  "scripts": {
    "test": "./node_modules/.bin/jshint . && ./node_modules/.bin/jsxhint . && babel-node --presets es2015 ./node_modules/.bin/_mocha $(find test -name '*.js')",
    "build": "webpack && npm run build-css",
    "build-js": "webpack",
    "build-css": "node-sass --include-path assets/sass assets/sass/main.scss dist/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "watch-js": "nodemon -e js -x \"npm run test && npm run build-js\"",
    "watch": "npm run watch-js && npm run watch-css",
    "prebuild": "npm run test"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "phaser-shim": "^2.4.8"
  }
}
